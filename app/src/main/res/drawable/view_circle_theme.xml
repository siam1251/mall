<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape
            android:shape="oval">
            <solid
                android:color="@color/colorPrimaryDark"/>
        </shape>
    </item>
    <item>
        <layer-list>
            <item>
                <shape
                    android:shape="oval">
                    <solid
                        android:color="@color/themeColor"/>
                </shape>
            </item>
        </layer-list>
    </item>

</selector>
